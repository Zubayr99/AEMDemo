<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:contextHubPath="/etc/cloudsettings/default/contexthub"
        cq:contextHubSegmentsPath="/etc/segmentation/contexthub"
        cq:template="/conf/aemtraining/settings/wcm/templates/content-page"
        jcr:mixinTypes="[cq:LiveRelationship,cq:LiveSync]"
        jcr:primaryType="cq:PageContent"
        jcr:title="en"
        sling:resourceType="aemtraining/components/structure/page"
        pageTitle="AEM DEMO">
        <root
                jcr:mixinTypes="[cq:LiveRelationship,cq:LiveSync]"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemtraining/components/container"
            layout="responsiveGrid">
            <container
                    jcr:mixinTypes="[cq:LiveRelationship,cq:LiveSync]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemtraining/components/container">
                <title
                        jcr:mixinTypes="[cq:LiveRelationship,cq:LiveSync]"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemtraining/components/title"/>
                <container
                        jcr:mixinTypes="[cq:LiveRelationship,cq:LiveSync]"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemtraining/components/container"
                    layout="responsiveGrid">
                    <teaser
                        jcr:description="&lt;p>Don't stop half way, go for the top!&lt;/p>&#xd;&#xa;"
                        jcr:mixinTypes="[cq:LiveRelationship,cq:LiveSync]"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Epic Journey"
                        sling:resourceType="aemtraining/components/teaser"
                        actionsEnabled="false"
                        descriptionFromPage="false"
                        textIsRich="true"
                        titleFromPage="false">
                        <cq:responsive
                                jcr:mixinTypes="[cq:LiveRelationship,cq:LiveSync]"
                                jcr:primaryType="nt:unstructured">
                            <default
                                    jcr:mixinTypes="[cq:LiveRelationship,cq:LiveSync]"
                                    jcr:primaryType="nt:unstructured"
                                    offset="0"
                                    width="4"/>
                        </cq:responsive>
                    </teaser>
                </container>
            </container>
        </root>
        <cq:LiveSyncConfig
                cq:isDeep="{Boolean}false"
                cq:master="/content/aemtraining/language-masters/en"
                cq:rolloutConfigs="[/libs/msm/wcm/rolloutconfigs/default]"
                jcr:primaryType="cq:LiveCopy"/>
    </jcr:content>
    <home/>
    <component-info/>
</jcr:root>
